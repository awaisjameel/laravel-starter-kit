        "clsx": "^2.1.1",
        "concurrently": "^9.2.1",
        "laravel-vite-plugin": "^2.0.1",
        "lucide-vue-next": "^0.562.0",
        "pinia": "^3.0.4",
        "reka-ui": "^2.7.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.1",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5.9.3",
        "vite": "^7.3.0",
        "vue": "^3.5.26",
        "ziggy-js": "^2.6.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.55.1",
